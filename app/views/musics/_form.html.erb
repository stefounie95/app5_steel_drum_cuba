<%= simple_form_for(@music, :html => {:multipart => true}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, label:"Nom :" %>
    <br>
    <%= f.label "Fichier audio :" %>
    <%= f.file_field :sound %>
  </div>
  <br>
  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-success' %>
  </div>
<% end %>
